# This is Git's per-user configuration file.
[push]
	default = simple
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
	pr = pull --rebase
	df = diff
[pull]
	rebase = true
[commit]
	verbose = true
	gpgsign = true
[core]
	excludesfile = /Users/joelvaneenwyk/.gitignore_global
